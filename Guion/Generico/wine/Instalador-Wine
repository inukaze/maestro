			sudo ln -sf "/usr/share/desktop-directories/wine-Programs-Accessories.directory" "/usr/local/share/desktop-directories/wine-Programs-Accessories.directory"
			sudo ln -sf "/usr/share/desktop-directories/wine-Programs.directory" "/usr/local/share/desktop-directories/wine-Programs.directory"
			sudo ln -sf "/usr/bin/function_grep.pl" "/usr/local/bin/function_grep.pl"
			sudo ln -sf "/usr/bin/msiexec" "/usr/local/bin/msiexec"
			sudo ln -sf "/usr/bin/notepad" "/usr/local/bin/notepad"
			sudo ln -sf "/usr/bin/regedit" "/usr/local/bin/regedit"
			sudo ln -sf "/usr/bin/regsvr32" "/usr/local/bin/regsvr32"
			sudo ln -sf "/usr/bin/widl" "/usr/local/bin/widl"
			sudo ln -sf "/usr/bin/wine" "/usr/local/bin/wine"
			sudo ln -sf "/usr/bin/wineboot" "/usr/local/bin/wineboot"
			sudo ln -sf "/usr/bin/winebuild" "/usr/local/bin/winebuild"
			sudo ln -sf "/usr/bin/winecfg" "/usr/local/bin/winecfg"
			sudo ln -sf "/usr/bin/wineconsole" "/usr/local/bin/wineconsole"
			sudo ln -sf "/usr/bin/winecpp" "/usr/local/bin/winecpp"
			sudo ln -sf "/usr/bin/winedbg" "/usr/local/bin/winedbg"
			sudo ln -sf "/usr/bin/winedump" "/usr/local/bin/winedump"
			sudo ln -sf "/usr/bin/winefile" "/usr/local/bin/winefile"
			sudo ln -sf "/usr/bin/wineg++" "/usr/local/bin/wineg++"
			sudo ln -sf "/usr/bin/winegcc" "/usr/local/bin/winegcc"
			sudo ln -sf "/usr/bin/winemaker" "/usr/local/bin/winemaker"
			sudo ln -sf "/usr/bin/winemine" "/usr/local/bin/winemine"
