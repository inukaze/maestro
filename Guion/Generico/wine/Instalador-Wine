		sudo rm -rf $WINE_SHARE_PREFIX/share/man/man1/winecfg.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/man1/wineconsole.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/man1/winecpp.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/man1/winedbg.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/man1/winedump.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/man1/winefile.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/man1/wineg++.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/man1/winegcc.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/man1/winemaker.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/man1/winemine.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/man1/winepath.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/man1/wineserver.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/man1/wmc.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/man1/wrc.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/pl.UTF-8/man1/wine.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/wine/fonts
		sudo rm -rf $WINE_SHARE_PREFIX/share/wine
		Arquitectura=$(uname -m)
			if        [ "$Arquitectura" = "amd64" ]; then
					sudo rm -rf $WINE_SHARE_PREFIX/lib/wine/fakedlls/
					sudo rm -rf $WINE_SHARE_PREFIX/lib/wine
					sudo rm -rf $WINE_SHARE_PREFIX/lib/libwine.so
					sudo rm -rf $WINE_SHARE_PREFIX/lib/libwine.so.1
					sudo rm -rf $WINE_SHARE_PREFIX/lib/libwine.so.1.0
					sudo rm -rf $WINE_SHARE_PREFIX/lib64/wine/fakedlls/
					sudo rm -rf $WINE_SHARE_PREFIX/lib64/wine
					sudo rm -rf $WINE_SHARE_PREFIX/lib64/libwine.so
					sudo rm -rf $WINE_SHARE_PREFIX/lib64/libwine.so.1
					sudo rm -rf $WINE_SHARE_PREFIX/lib64/libwine.so.1.0

						if test -d "$RUTACTUAL"/lib64
							then
								sudo rm -rf	"$WINE_SHARE_PREFIX"/lib64/wine
								sudo mkdir -p	"$WINE_SHARE_PREFIX"/lib64/wine
								sudo cp -rf	"$RUTACTUAL"/lib64/wine/** "$WINE_SHARE_PREFIX"/lib64/wine
							fi
						
						if test -d /usr/lib/x86_64-linux-gnu
							then								
								sudo rm -rf	/usr/lib/x86_64-linux-gnu/wine							
								sudo mkdir -p	/usr/lib/x86_64-linux-gnu/wine						
								sudo cp -rf	"$RUTACTUAL/lib64"/wine/** /usr/lib/x86_64-linux-gnu/wine
						fi

						if [ -d /etc/ld.so.conf.d ]
						then
							sudo chmod 777 /etc/ld.so.conf.d
							if [ -d /lib32 ] || [ -d /usr/lib32 ]							
							then
							echo
									if [ -e /etc/ld.so.conf.d/i686-linux-gnu.conf ]; then
										sudo cp /etc/ld.so.conf.d/i686-linux-gnu.conf /etc/ld.so.conf.d/i686-linux-gnu.conf.original
										sudo sed '/lib32/d' /etc/ld.so.conf.d/i686-linux-gnu.conf >> /etc/ld.so.conf.d/i686-linux-gnu.conf.inukaze
										sudo mv /etc/ld.so.conf.d/i686-linux-gnu.conf.inukaze /etc/ld.so.conf.d/i686-linux-gnu.conf
										echo "/lib32" >> /etc/ld.so.conf.d/i686-linux-gnu.conf
										echo "/usr/lib32" >> /etc/ld.so.conf.d/i686-linux-gnu.conf	
									elif [ -e /etc/ld.so.conf.d/lib32-glibc.conf ]; then
										sudo cp /etc/ld.so.conf.d/lib32-glibc.conf /etc/ld.so.conf.d/lib32-glibc.conf.original
										sudo sed '/lib32/d' /etc/ld.so.conf.d/lib32-glibc.conf >> /etc/ld.so.conf.d/lib32-glibc.conf.inukaze
										sudo mv /etc/ld.so.conf.d/lib32-glibc.conf.inukaze /etc/ld.so.conf.d/lib32-glibc.conf
										echo "/lib32" >> /etc/ld.so.conf.d/lib32-glibc.conf
										echo "/usr/lib32" >> /etc/ld.so.conf.d/lib32-glibc.conf					
									fi
							fi
							sudo chmod 755 /etc/ld.so.conf.d
						fi						
			fi
				
		sudo cp -rf "$RUTACTUAL"/bin/* $WINE_SHARE_PREFIX/bin
		sudo cp -rf "$RUTACTUAL"/lib/* $WINE_SHARE_PREFIX/lib
		sudo cp -rf "$RUTACTUAL"/share/* $WINE_SHARE_PREFIX/share
	
		if test -d /usr/lib/i386-linux-gnu
			then
				sudo rm -rf /usr/lib/i386-linux-gnu/wine
				sudo mkdir -p /usr/lib/i386-linux-gnu/wine								
				sudo cp -rf "$RUTACTUAL"/lib/** /usr/lib/i386-linux-gnu/wine
		fi

		if test -d /lib
			then
				sudo rm -rf /lib32/wine
				sudo ln -sf /usr/lib/i386-linux-gnu/wine /lib32/wine
		fi

		if test -d /lib32
			then
				sudo rm -rf /lib32/wine
				sudo ln -sf /usr/lib/i386-linux-gnu/wine /lib32/wine
		fi
						
		if test -d /usr/lib32
			then
				sudo rm -rf /usr/lib32/wine
				sudo ln -sf /usr/lib/i386-linux-gnu/wine /usr/lib32/wine
		fi
	
		cd ../..
		sudo rm -rf wineversion
		cd ..
		RUTACTUAL="$PWD"
		#Descargar Iconos , Extraerlos
		#Coloarlos en /usr/share/icons/hicolor
		sudo mkdir -p /usr/share/icons/hicolor &> /dev/null
		cd /usr/share/icons/hicolor
		sudo wget https://dl.dropboxusercontent.com/u/3164499/Linux/wine-icons.tar.gz &> /dev/null
		sudo tar xfvz wine-icons.tar.gz &> /dev/null
		sudo rm -rf wine-icons.tar.gz
		cd /usr/share/icons
		2>/dev/null 1>/dev/null find . -type d -maxdepth 1 | while read DIRNAME; do sudo update-icon-caches "${DIRNAME}"; done &> /dev/null
		cd "$RUTACTUAL"
		
		# Crear Directorios & Enlaces:
		sudo mkdir -p /etc/xdg/menus/applications-merged
		sudo mkdir -p /usr/share/desktop-directories
		echo '<!DOCTYPE Menu PUBLIC "-//freedesktop//DTD Menu 1.0//EN"
			"http://www.freedesktop.org/standards/menu-spec/menu-1.0.dtd">
			<Menu>
			  <Name>Applications</Name>
			  <Menu>
