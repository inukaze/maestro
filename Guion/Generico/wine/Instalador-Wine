			sudo ln -sf "/usr/lib/wine" "/usr/local/lib/wine"
			sudo ln -sf "/usr/share/applications/wine.desktop" "/usr/local/share/applications/wine.desktop"
			sudo ln -sf "/usr/share/applications/wine-winecfg.desktop" "/usr/local/share/applications/wine-winecfg.desktop"
