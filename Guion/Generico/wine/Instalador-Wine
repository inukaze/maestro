							then								
								sudo rm -rf	/usr/lib/x86_64-linux-gnu/wine							
								sudo mkdir -p	/usr/lib/x86_64-linux-gnu/wine						
								sudo cp -rf	"$RUTACTUAL/lib64"/wine/** /usr/lib/x86_64-linux-gnu/wine
						fi
					
						if [ -d /etc/ld.so.conf.d ]
						then
							sudo chmod 777 /etc/ld.so.conf.d
							if [ -d /lib32 ] || [ -d /usr/lib32 ]							
							then
							echo
									if [ -e /etc/ld.so.conf.d/i686-linux-gnu.conf ]; then
										sudo cp /etc/ld.so.conf.d/i686-linux-gnu.conf /etc/ld.so.conf.d/i686-linux-gnu.conf.original
										sudo sed '/lib32/d' /etc/ld.so.conf.d/i686-linux-gnu.conf >> /etc/ld.so.conf.d/i686-linux-gnu.conf.inukaze
										sudo mv /etc/ld.so.conf.d/i686-linux-gnu.conf.inukaze /etc/ld.so.conf.d/i686-linux-gnu.conf
										echo "/lib32" >> /etc/ld.so.conf.d/i686-linux-gnu.conf
										echo "/usr/lib32" >> /etc/ld.so.conf.d/i686-linux-gnu.conf	
									elif [ -e /etc/ld.so.conf.d/lib32-glibc.conf ]; then
										sudo cp /etc/ld.so.conf.d/lib32-glibc.conf /etc/ld.so.conf.d/lib32-glibc.conf.original
										sudo sed '/lib32/d' /etc/ld.so.conf.d/lib32-glibc.conf >> /etc/ld.so.conf.d/lib32-glibc.conf.inukaze
										sudo mv /etc/ld.so.conf.d/lib32-glibc.conf.inukaze /etc/ld.so.conf.d/lib32-glibc.conf
										echo "/lib32" >> /etc/ld.so.conf.d/lib32-glibc.conf
										echo "/usr/lib32" >> /etc/ld.so.conf.d/lib32-glibc.conf					
									fi
							fi
							sudo chmod 755 /etc/ld.so.conf.d
						fi 
			fi
				
		sudo cp -rf "$RUTACTUAL/bin"/** $WINE_SHARE_PREFIX/bin/
		sudo cp -rf "$RUTACTUAL/lib"/** $WINE_SHARE_PREFIX/lib/
		sudo cp -rf "$RUTACTUAL/share"/** $WINE_SHARE_PREFIX/share/
		
		if test -d /usr/lib/i386-linux-gnu
			then
				sudo rm -rf /usr/lib/i386-linux-gnu/wine
				sudo mkdir -p /usr/lib/i386-linux-gnu/wine								
				sudo cp -rf "$RUTACTUAL/lib"/** /usr/lib/i386-linux-gnu/wine/
		fi

		if test -d /lib
			then
				sudo rm -rf /lib/wine
				sudo ln -sf /usr/lib/i386-linux-gnu/wine /lib/wine
		fi

		if test -d /lib32
			then
				sudo rm -rf /lib32/wine
				sudo ln -sf /usr/lib/i386-linux-gnu/wine /lib32/wine
		fi
						
