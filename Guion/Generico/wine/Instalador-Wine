		sudo rm -rf $WINE_SHARE_PREFIX/share/man/de.UTF-8/man1/wineserver.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/fr.UTF-8/man1/wine.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/fr.UTF-8/man1/winemaker.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/fr.UTF-8/man1/wineserver.1
		sudo rm -rf $WINE_SHARE_PREFIX/share/man/man1/msiexec.1
