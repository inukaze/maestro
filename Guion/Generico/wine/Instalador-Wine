			sudo ln -sf "/usr/bin/winegcc" "/usr/local/bin/winegcc"
			sudo ln -sf "/usr/bin/winemaker" "/usr/local/bin/winemaker"
			sudo ln -sf "/usr/bin/winemine" "/usr/local/bin/winemine"
			sudo ln -sf "/usr/bin/winepath" "/usr/local/bin/winepath"
			sudo ln -sf "/usr/bin/wine-preloader" "/usr/local/bin/wine-preloader"
			sudo ln -sf "/usr/bin/wineserver" "/usr/local/bin/wineserver"
			sudo ln -sf "/usr/bin/wmc" "/usr/local/bin/wmc"
			sudo ln -sf "/usr/bin/wrc" "/usr/local/bin/wrc"
			if        [ "$Arquitectura" = "amd64" ]; then
				sudo mkdir -p "/usr/local/lib64" &> /dev/null
				sudo ln -sf "/usr/lib64/wine" "/usr/local/lib64/wine"
				sudo ln -sf "/usr/lib64/libwine.so" "/usr/local/lib64/libwine.so"
				sudo ln -sf "/usr/lib64/libwine.so.1" "/usr/local/lib64/libwine.so.1"
				sudo ln -sf "/usr/lib64/libwine.so.1.0" "/usr/local/lib64/libwine.so.1.0"			
				if [ -f /usr/bin/wine64 ] || [ -f /usr/bin/wine64-preloader ]
				then
					sudo ln -sf "/usr/bin/wine64" "/usr/local/bin/wine64"
					sudo ln -sf "/usr/bin/wine64-preloader" "/usr/local/bin/wine64-preloader"
				fi
			fi
		fi
	fi

    cd "$DIRORIGEN"
    if test ! -f PlayOnLinux-wine-"$Opcion-linux-$Arquitectura".pol
    then
	cd "$DIRORIGEN"
        wget http://www.playonlinux.com/wine/binaries/linux-$Arquitectura/PlayOnLinux-wine-"$Opcion-linux-$Arquitectura".pol
        SUMA_POL_OBTENIDA=$(sha1sum < PlayOnLinux-wine-"$Opcion-linux-$Arquitectura".pol | sed 's/(stdin)= //;s/ .*//')
	        if [ "$SUMA_POL_OBTENIDA"x != "$VersionWine_SHA1SUM"x ]
        		then
printf "\n"
sudo echo "Suma Conseguida : $SUMA_POL_OBTENIDA " $'\n'
sudo echo "Suma Necesaria  : $VersionWine_SHA1SUM " $'\n'
sudo echo "Integridad Incorrecta. Vuelve a Intentarlo" $'\n'
printf "\n"
        	fi

		WINE_SHARE_PREFIX=/usr
		2>/dev/null 1>/dev/null tar xfvj PlayOnLinux-wine-"$Opcion-linux-$Arquitectura".pol
		rm -rf playonlinux files
		sudo chmod a+o+x -R wineversion/"$Opcion"
		cd wineversion/"$Opcion"
