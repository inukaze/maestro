# Determinando la arquitectura de la maquina
Arquitectura=$(uname -m)
case "$Arquitectura" in
    x86)	Arquitectura="x86"					;;
    i?86)	Arquitectura="x86"					;;
    amd64)	Arquitectura="amd64"					;;
    x86_64)	Arquitectura="x86_64"					;;
* ) echo    "Tu Arquitectura '$Arquitectura' -> NO ESTA SOPORTADA."	;;
esac

Instalar_Wine(){

if [	"$Arquitectura" = "x86_64" ]; then
	  Arquitectura="amd64"
fi

    case $1 in
    1.7.32)
        VersionWine=1.7.32
	case "$Arquitectura" in
        x86)    VersionWine_SHA1SUM=d3ff8af93ecf258504524747fa0732cc60ac5586;;
        amd64) VersionWine_SHA1SUM=227be1c78376af80fda803fead29f823a17ff06b;;
        esac
        ExtensionVersionWine=.pol
	export VersionWine
	export ExtensionVersionWine
        ;;
    1.7.31)
        VersionWine=1.7.31
	case "$Arquitectura" in
        x86)    VersionWine_SHA1SUM=444b7222b6798b91cb57bf8859a34566664c6232;;
        amd64) VersionWine_SHA1SUM=06591df5e5a4d01a8a9f0e44a4efd35efa0e0f5d;;
        esac
        ExtensionVersionWine=.pol
	export VersionWine
	export ExtensionVersionWine
        ;;
    1.7.30)
        VersionWine=1.7.30
	case "$Arquitectura" in
        x86)    VersionWine_SHA1SUM=e53fcb721913deeca0bd9ae017b9ab379e75adef;;
        amd64) VersionWine_SHA1SUM=d13240f8b73c5f7f39b999b8d848937ed178dabf;;
        esac
        ExtensionVersionWine=.pol
	export VersionWine
	export ExtensionVersionWine
        ;;
    1.7.29)
        VersionWine=1.7.29
	case "$Arquitectura" in
        x86)    VersionWine_SHA1SUM=5740ccbe2a620f929368b44cace7bd1b876585ba;;
        amd64) VersionWine_SHA1SUM=1620d8ca538a5bdd276be8ee390f1eb38825c663;;
        esac
        ExtensionVersionWine=.pol
	export VersionWine
	export ExtensionVersionWine
        ;;
    1.7.28)
        VersionWine=1.7.28
	case "$Arquitectura" in
        x86)    VersionWine_SHA1SUM=00ed128dd78bb497038f2fe5ab2325c5bd0f558f;;
        amd64) VersionWine_SHA1SUM=3459b94b7daf644ef5e8962f296602f40c615e64;;
        esac
        ExtensionVersionWine=.pol
	export VersionWine
	export ExtensionVersionWine
        ;;
    1.7.27)
        VersionWine=1.7.27
	case "$Arquitectura" in
        x86)    VersionWine_SHA1SUM=4d6b42314cf07900bb93b1131b61abb11d20fa43;;
        amd64) VersionWine_SHA1SUM=4cdddbd0a2480b7010d2edf9e855d792c9796f8b;;
        esac
        ExtensionVersionWine=.pol
	export VersionWine
	export ExtensionVersionWine
        ;;
    1.7.26)
        VersionWine=1.7.26
	case "$Arquitectura" in
        x86)    VersionWine_SHA1SUM=91feb35036277c287e28f3596b187d672c1c6e38;;
        amd64) VersionWine_SHA1SUM=40dd034d0affa114675ad065d465cadca1e0c92e;;
        esac
        ExtensionVersionWine=.pol
	export VersionWine
	export ExtensionVersionWine
        ;;
    1.7.25)
	VersionWine=1.7.25
	case "$Arquitectura" in
        x86)    VersionWine_SHA1SUM=def218117f1c486bf1a434147936ee708620e050;;
        amd64) VersionWine_SHA1SUM=4cd32371a6c3a8f40cc50a1f0b3926c9fea8589a;;
        esac
        ExtensionVersionWine=.pol
	export VersionWine
	export ExtensionVersionWine
        ;;
    1.7.24)
        VersionWine=1.7.24
	case "$Arquitectura" in
        x86)    VersionWine_SHA1SUM=47feb3a54ed7b72cfd03a0d43bd69fe4e6312b43;;
        amd64) VersionWine_SHA1SUM=31ac55aad169d36b0e10ec4069b91ea29dd879f8;;
        esac
        ExtensionVersionWine=.pol
	export VersionWine
	export ExtensionVersionWine
        ;;
    1.7.23)
        VersionWine=1.7.23
	case "$Arquitectura" in
        x86)    VersionWine_SHA1SUM=69981f0c11d45b98822fd8e8ab557eafab3f0011;;
        amd64) VersionWine_SHA1SUM=fbc387975c45b98fbda1fc2d5b40a93ae09b25cc;;
        esac
        ExtensionVersionWine=.pol
	export VersionWine
	export ExtensionVersionWine
        ;;
    1.7.22)
        VersionWine=1.7.22
	case "$Arquitectura" in
        x86)    VersionWine_SHA1SUM=2b1432d7e42faaf1e607359ce5e73700c7aab334;;
        amd64) VersionWine_SHA1SUM=2078208b4a5f268fcff4d68c747ccb183afdaac3;;
        esac
        ExtensionVersionWine=.pol
	export VersionWine
	export ExtensionVersionWine
        ;;
